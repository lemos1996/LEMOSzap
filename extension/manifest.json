{
   "action": {
      "default_icon": "label/icons/plugin/icon.png"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [
      {
         "css": [
            "label/css/lemoszap.css",
            "content/assets/css/style.css",
            "content.css"
         ],
         "js": [
            "content/index.js"
         ],
         "matches": [
            "https://web.whatsapp.com/*"
         ]
      }
   ],
   "description": "LEMOSzap é um CRM completo para WhatsApp Web. Organize seus chats e otimize seus negócios no WhatsApp Web",
   "externally_connectable": {
      "matches": [
         "https://web.whatsapp.com/*"
      ]
   },
   "icons": {
      "128": "label/icons/plugin/icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "LEMOSzap - CRM para WhatsApp Web",
   "permissions": [
      "unlimitedStorage",
      "storage",
      "alarms",
      "tabs"
   ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "7.4.3.6",
   "web_accessible_resources": [
      {
         "matches": [
            "https://web.whatsapp.com/*"
         ],
         "resources": [
            "*.js",
            "*.css",
            "*.png",
            "*.mp3",
            ".ttf",
            "label/*.json",
            "label/*.png"
         ]
      }
   ]
}